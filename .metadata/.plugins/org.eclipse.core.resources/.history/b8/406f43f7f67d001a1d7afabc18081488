package com.accomodation.model;

public class RoomModel {
	 protected int id;
	 protected int typeid;
	    protected String typename;
	    protected String available;
	    protected String location;
	    protected int monthlycharge;
	    
	    public RoomModel(int id,int type,String typename,String available,String location, int monthlycharge) {
	    	this.typename=typename;
	    	this.available=available;
	    	this.id=id;
	    	this.typeid=type;
	    	this.location=location;
	    	this.monthlycharge=monthlycharge;
	    }
	    public RoomModel(int type,String typename,String available,String location, int monthlycharge) {
	    	this.typename=typename;
	    	this.available=available;
	    	this.typeid=type;
	    	this.location=location;
	    	this.monthlycharge=monthlycharge;
	    }
	    
	    public int getId() {
	        return id;
	    }
	 
	    public void setId(int id) {
	        this.id = id;
	    }
	    public int getTypeID() {
	        return typeid;
	    }
	 
	    public void setTypeId(int typeid) {
	        this.typeid = typeid;
	    }
	    public int getMonthlyCharge() {
	        return monthlycharge;
	    }
	 
	    public void setMonthlyCharge(int id) {
	        this.id = id;
	    }
}
